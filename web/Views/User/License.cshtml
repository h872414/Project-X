@{
    ViewBag.Title = "License adatok";
}
@model DicomLoaderWeb.Models.User 


<!--Main-->
<form class="bg-light p-3 my-5 col-9 container-fluid">
    <h1 class="h2 text-center">License adatok</h1>
    <div class="form-group">
        <label asp-for="LastName">Vezetéknév</label>
        <input type="text" asp-for="LastName" id="lastName" class="form-control"  disabled>
    </div>
    <div class="form-group">
        <label  asp-for="FirstName">Keresztnév</label>
        <input type="text"  asp-for="FirstName" id="firstName" class="form-control" disabled>
    </div>
    <div class="form-group">
        <label asp-for="Email">Email cím</label>
        <input type="email" asp-for="Email" class="form-control" id="email" disabled>
    </div>
    <div class="form-group">
        <label  asp-for="RegDate">License érvényesség</label>
        <input type="text" asp-for="RegDate" class="form-control" id="license" disabled>
    </div>
</form>
<!--/Main-->